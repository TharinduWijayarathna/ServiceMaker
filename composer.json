{
    "name": "tharindu/service-maker",
    "description": "A Laravel package to generate service files via Artisan command.",
    "keywords": ["laravel", "service", "generator"],
    "license": "MIT",
    "authors": [
        {
            "name": "Tharindu Wijayarathna",
            "email": "wikum.dev@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Tharindu\\ServiceMaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tharindu\\ServiceMaker\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tharindu\\ServiceMaker\\YourPackageServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "test": "phpunit"
    }
}
